+import static org.junit.jupiter.api.Assertions.assertDoesNotThrow;
+import static org.junit.jupiter.api.Assertions.assertTrue;
+
+import edu.ntnu.idi.idatt.model.Dice;
+import org.junit.jupiter.api.BeforeEach;
+import org.junit.jupiter.api.Test;
+
 public class DiceTest {
+  private Dice dice;
+
+
+  @BeforeEach
+  void setUp() {
+    dice = new Dice(2);
+    dice.roll();
+  }
+
+  @Test
+  void testRollingMultipleDice() {
+    for (int i = 0; i < 100; i++) {
+      int value = dice.roll();
+      assertTrue(value >= 0 && value <= 12, "Dice value rolled: " +value+ "but must be between 0 and 12");
+    }
+  }
+
+  @Test
+  void testGetDieValue() {
+    assertDoesNotThrow(() -> dice.getDieValue(0));
+    assertDoesNotThrow(() -> dice.getDieValue(1));
+
+  }
+
+
 
 }
